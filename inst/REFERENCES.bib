 journal = {{Journal of circadian rhythms}},
 doi = {10.1186/1740-3391-8-2},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/20377841},
 file = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC2851666},
 file = {Rea, Figueiro et al. 2010 - Circadian light:H\:\\Promotion\\Literatur\\Citavi 6\\Citavi Attachments\\Rea, Figueiro et al. 2010 - Circadian light.PDF:PDF;Rea, Figueiro et al. 2010 - Circadian light (2):H\:\\Promotion\\Literatur\\Citavi 6\\Citavi Attachments\\Rea, Figueiro et al. 2010 - Circadian light (2).pdf:pdf}
