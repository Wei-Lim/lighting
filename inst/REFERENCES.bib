@article{Lucas2014,
 abstract = {Light is a potent stimulus for regulating circadian, hormonal, and behavioral systems. In addition, light therapy is effective for certain affective disorders, sleep problems, and circadian rhythm disruption. These biological and behavioral effects of light are influenced by a distinct photoreceptor in the eye, melanopsin-containing intrinsically photosensitive retinal ganglion cells (ipRGCs), in addition to conventional rods and cones. We summarize the neurophysiology of this newly described sensory pathway and consider implications for the measurement, production, and application of light. A new light-measurement strategy taking account of the complex photoreceptive inputs to these non-visual responses is proposed for use by researchers, and simple suggestions for artificial/architectural lighting are provided for regulatory authorities, lighting manufacturers, designers, and engineers.},
 author = {Lucas, Robert J. and Peirson, Stuart N. and Berson, David M. and Brown, Timothy M. and Cooper, Howard M. and Czeisler, Charles A. and Figueiro, Mariana G. and Gamlin, Paul D. and Lockley, Steven W. and O Hagan, John B. and Price, Luke L. A. and Provencio, Ignacio and Skene, Debra J. and Brainard, George C.},
 year = {2014},
 title = {{Measuring and using light in the melanopsin age}},
 keywords = {{\#}20191114{\_}LRT Pulikation;{\#}20200717{\_}B{\&}E Publikation;{\#}R package lighting;Animals;Circadian Rhythm/physiology;Humans;Photoreceptor Cells/metabolism;Phototherapy/trends;Retinal Ganglion Cells/metabolism;Rod Opsins/physiology},
 pages = {1--9},
 volume = {37},
 number = {1},
 journal = {{Trends in neurosciences}},
 doi = {10.1016/j.tins.2013.10.004},
 file = {Lucas, Peirson et al. 2014 - Measuring and using light (2):H\:\\Promotion\\Literatur\\Citavi 6\\Citavi Attachments\\Lucas, Peirson et al. 2014 - Measuring and using light (2).pdf:pdf;Lucas, Peirson et al. 2014 - Measuring and using light:H\:\\Promotion\\Literatur\\Citavi 6\\Citavi Attachments\\Lucas, Peirson et al. 2014 - Measuring and using light.pdf:pdf;Lucas, Peirson et al. 2014 - Measuring and using light:H\:\\Promotion\\Literatur\\Citavi 6\\Citavi Attachments\\Lucas, Peirson et al. 2014 - Measuring and using light.xls:xls}
}


@article{Rea2010,
 abstract = {The present paper reflects a work in progress toward a definition of circadian light, one that should be informed by the thoughtful, century-old evolution of our present definition of light as a stimulus for the human visual system. This work in progress is based upon the functional relationship between optical radiation and its effects on nocturnal melatonin suppression, in large part because the basic data are available in the literature. Discussed here are the fundamental differences between responses by the visual and circadian systems to optical radiation. Brief reviews of photometry, colorimetry, and brightness perception are presented as a foundation for the discussion of circadian light. Finally, circadian light (CLA) and circadian stimulus (CS) calculation procedures based on a published mathematical model of human circadian phototransduction are presented with an example.},
 author = {Rea, Mark S. and Figueiro, Mariana G. and Bierman, Andrew and Bullough, John D.},
 year = {2010},
 title = {{Circadian light}},
 keywords = {{\#}20191114{\_}LRT Pulikation;{\#}20200717{\_}B{\&}E Publikation;{\#}R package lighting},
 pages = {2},
 volume = {8},
